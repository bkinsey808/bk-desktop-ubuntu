#!/usr/bin/env xdg-open
[Desktop Entry]
Version=1.0
Name=Gallery
GenericName=Ubuntu Photo Viewer
X-GNOME-Fullname=Ubuntu Photo Viewer
Comment=Browse your photographs
Exec=gallery-app %u
Icon=gallery-app
StartupNotify=true
Terminal=false
Type=Application
MimeType=image/jpg;image/jpeg;
Categories=Graphics;Photography;RasterGraphics;Viewer;
X-Ayatana-Appmenu-Show-Stubs=false
X-Ubuntu-Touch=true
X-Ubuntu-Gettext-Domain=gallery-app
X-Ubuntu-Single-Instance=true
